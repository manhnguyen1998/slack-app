{
  "manifest_version": 3,
  "name": "Slackify",
  "description": "Send link to your slack channel with a single click!",
  "version": "1.0.0",
  "action": { "default_popup": "index.html" },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "slack16.png",
    "48": "slack48.png",
    "128": "slack128.png"
  }

}
